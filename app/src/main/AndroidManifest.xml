        <activity android:name=".activity.ActivityOnSwipe" />
        <activity android:name=".activity.ActivityKeyCircle" />
        <activity android:name=".activity.ActivityRecyclerViewFull" />
        <activity android:name=".activity.ActivityViewPagerAni" />
        <activity android:name=".activity.DetailActivity" />
        <activity android:name=".activity.RecyclerAnimation" />
        <activity android:name=".activity.ResizeText" />
        <activity android:name=".activity.ImageSwipAndResize" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>